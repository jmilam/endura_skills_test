<%= form_for :user_answer, as: :user_answer, url: user_answers_part_one_index_path, method: :create do |f| %>
	<div id="user">
		<div class="text-center">
			<div class="col-md-5 col-md-offset-3">
				<div class="panel panel-danger">
				  <div class="panel-heading"><h4>Sign In</h4></div>
				  <div class="panel-body">
				  	<div class="col-md-12">
					    <div class="row">
								<div class="field">
						      <%= f.label :first_name %><br />
						      <%= f.text_field :first_name, autofocus: true, autocomplete: "off", id: "first_name", class: "form-control" %>
						    </div>
						  </div>
						  <div class="row">
						    <div class="field">
						      <%= f.label :last_name %><br />
						      <%= f.text_field :last_name, autocomplete: "off", id: "last_name",  class: "form-control" %>
						    </div>
							</div>
							<div class="row">
						    <div class="field">
						      <%= f.label :email %><br />
						      <%= f.email_field :email, autocomplete: "off", id: "email",  class: "form-control" %>
						      <%= f.hidden_field :user_id, id: "user_id" %>
						    </div>
						  </div>
							<div class="row">
							<%= link_to "Next", "#user", class: "btn btn-success next", id: "user" %>
							</div>
						</div>	
				  </div>
				</div>
			</div>
		</div>
	</div>

